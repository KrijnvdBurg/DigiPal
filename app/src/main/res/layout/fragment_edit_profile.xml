<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gsc.DigiPal.activities.fragments.editProfile"
    android:background="#929292">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <View
                android:layout_width="72.5dp"
                android:layout_height="2dp"
                android:background="#c0c0c0"
                android:layout_alignBottom="@+id/strength_ratingBar"
                android:layout_alignParentStart="true"
                android:layout_alignEnd="@+id/strength_ratingBar"
                android:layout_marginTop="10dp" />

            <RatingBar
                android:id="@+id/strength_ratingBar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:numStars="10"
                android:stepSize="1.0"
                android:rating="2.0"
                style="@style/foodRatingBar"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                />

        </FrameLayout>

    </RelativeLayout>

</FrameLayout>
